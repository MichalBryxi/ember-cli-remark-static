{
  "name": "ember-cli-remark-static",
  "version": "3.0.4",
  "description": "Markdown for Ember.js with component support",
  "keywords": [
    "ember-addon",
    "markdown",
    "remark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ampatspell/ember-cli-remark-static.git"
  },
  "license": "MIT",
  "author": "Arnis Vuskans <ampatspell@gmail.com> (http://www.amateurinmotion.com/)",
  "funding": {
    "url": "https://github.com/sponsors/ampatspell"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "WITH_FASTBOOT=true ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "bump": "npm version minor; git push --follow-tags",
    "patch": "npm version patch; git push --follow-tags",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^3.0.2",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-fetch": "^8.0.4",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "macro-decorators": "^0.1.2",
    "rehype-raw": "^5.1.0",
    "remark-breaks": "^2.0.2",
    "remark-highlight.js": "^6.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "simple-dom": "^1.4.0",
    "tracked-toolbox": "^1.2.3",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.25.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.0.5",
    "prember-crawler": "^1.0.0",
    "prember-middleware": "^0.1.0",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "sass": "^1.32.8"
  },
  "fastbootDependencies": [
    "simple-dom"
  ],
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
